{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  packageRules: [
    {
      commitMessagePrefix: 'build(deps): ',
      matchPackageNames: [
        '/.*/',
      ],
    },
    {
      matchUpdateTypes: [
        'patch',
      ],
      matchCurrentVersion: '!/^0/',
      automerge: true,
      automergeType: 'pr',
      ignoreTests: false,
      platformAutomerge: true,
      matchPackageNames: [
        '/.*/',
      ],
    },
    {
      matchFileNames: ["renovate.json5"],
      commitMessagePrefix: "build(renovate): ",
    },
  ],
}
